    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "autoprefixer": "^9.8.0",
    "babel-loader": "^8.1.0",
    "chalk": "^4.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "compression": "^1.7.4",
    "compression-webpack-plugin": "^4.0.0",
    "cpx": "^1.5.0",
    "css-loader": "^3.5.3",
    "cssnano": "^4.1.10",
    "express": "^4.17.1",
    "favicons-webpack-plugin": "^3.0.1",
