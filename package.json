{
  "name": "react-redux",
  "version": "1.0.0",
  "description": "React template with basic logic and Redux on custom webpack builder",
  "main": "package.json",
  "scripts": {
    "start": "webpack-dev-server --config=./webpack.dev.js",
    "build": "webpack --config=./webpack.prod.js && npm run copy",
    "serve": "node server.js --open google-chrome",
    "copy": "cpx \"src/static/robots.txt\" \"dist/\"",
    "deploy": "npm run build && gh-pages -d dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EvgenyLeukhin/react-redux.git"
  },
  "keywords": [
    "react",
    "redux",
    "react-template"
  ],
  "author": "Evgeny Leukhin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EvgenyLeukhin/react-redux/issues"
  },
  "homepage": "https://github.com/EvgenyLeukhin/react-redux",
  "dependencies": {
    "axios": "^0.19.2",
    "bootstrap": "^4.5.0",
    "gh-pages": "^2.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "reactstrap": "^8.4.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
